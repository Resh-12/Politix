                        android:layout_width="80dp"
                        android:layout_height="100dp"
                        android:layout_marginLeft="8dp"
                        android:src="@drawable/reshma"
                        android:background="@drawable/circular_img" />

                    <TextView
                        android:id="@+id/vp1name"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_toRightOf="@+id/vp1img"
                        android:layout_marginLeft="100dp"
                        android:layout_marginTop="10dp"
                        android:textSize="18sp"
                        android:textStyle="bold"
                        android:text="reshma" />

                    <TextView
                        android:id="@+id/vp1cnt"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/vp1name"
                        android:layout_toRightOf="@+id/vp1img"
                        android:layout_marginLeft="100dp"
                        android:layout_marginTop="10dp"
                        android:textStyle="bold"
                        android:textSize="18sp"
                        android:text="12" />
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="10dp"
                    android:id="@+id/vp2lay"
                    android:background="@drawable/layout_corner"
                    android:layout_marginBottom="8dp">

                    <de.hdodenhof.circleimageview.CircleImageView
                        android:id="@+id/vp2img"
                        android:layout_width="80dp"
