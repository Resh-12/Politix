                    android:layout_marginLeft="20dp"
                    android:textSize="18sp"
                    android:textStyle="bold" />
                <!--                <androidx.recyclerview.widget.RecyclerView-->
                <!--                    android:layout_width="match_parent"-->
                <!--                    android:layout_height="wrap_content"-->
                <!--                    android:id="@+id/recyclesec"-->
                <!--                    android:layout_marginTop="10dp"/>-->
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="10dp"
                    android:id="@+id/seclay"
                    android:background="@drawable/layout_corner"
                    android:layout_marginBottom="8dp">

                    <de.hdodenhof.circleimageview.CircleImageView
                        android:id="@+id/sec1img"
                        android:layout_width="80dp"
                        android:layout_height="100dp"
                        android:layout_marginLeft="8dp"
                        android:src="@drawable/gou"
                        android:background="@drawable/circular_img" />

                    <TextView
                        android:id="@+id/sec1name"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_toRightOf="@+id/sec1img"
                        android:layout_marginLeft="100dp"
                        android:layout_marginTop="10dp"
                        android:textSize="18sp"
                        android:textStyle="bold"
                        android:text="gautham" />

                    <TextView
                        android:id="@+id/sec1cnt"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/sec1name"
                        android:layout_toRightOf="@+id/sec1img"
                        android:layout_marginLeft="100dp"
                        android:layout_marginTop="10dp"
                        android:textStyle="bold"
                        android:textSize="18sp"
                        android:text="8" />
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="10dp"
                    android:id="@+id/sec2lay"
                    android:background="@drawable/layout_corner"
                    android:layout_marginBottom="8dp">

                    <de.hdodenhof.circleimageview.CircleImageView
                        android:id="@+id/sec2img"
                        android:layout_width="80dp"
